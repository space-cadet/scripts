<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.deepak.screenshot-compressor</string>
    <key>ProgramArguments</key>
    <array>
        <string>/Users/deepak/code/scripts/compressor/compress-screenshots.sh</string>
    </array>
    <key>QueueDirectories</key>
    <array>
        <string>/Users/deepak/Screenshots</string>
    </array>
    <key>RunAtLoad</key>
    <false/>
    <key>StandardOutPath</key>
    <string>/tmp/screenshot-compressor.log</string>
    <key>StandardErrorPath</key>
    <string>/tmp/screenshot-compressor-error.log</string>
</dict>
</plist>
